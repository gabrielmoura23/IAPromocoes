    <!-- BEGIN PAGE CONTAINER -->
    <div class="page-container">
        <!-- BEGIN PAGE CONTENT -->
        @RenderBody()
        <!-- END PAGE CONTENT -->
    </div>
    <!-- END PAGE CONTAINER -->
