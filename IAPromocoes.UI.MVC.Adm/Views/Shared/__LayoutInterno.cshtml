                        </li>
                        <li>
                            <a href="javascript:;" data-original-title="vimeo" class="vimeo"></a>
                        </li>
                    </ul>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12 footer-block">
                    <h2>Contatos</h2>
                    <address class="margin-bottom-40">
                        Telefone: 71 9999 9999<br>
                        E-mail: <a href="mailto:contato@iapromocoes.com">contato@iapromocoes.com</a>
                    </address>
                </div>
                <div class="col-md-3 col-sm-6 col-xs12 footer-block">
                    &nbsp;
                </div>
            </div>
        </div>
    </div>
    <!-- END PRE-FOOTER -->
    <!-- BEGIN FOOTER -->
    <div class="page-footer">
        <div class="container-fluid">
            @DateTime.Now.Year &copy; IA Promoções. Todos os direitos reservados.
        </div>
    </div>
    <div class="scroll-to-top">
        <i class="icon-arrow-up"></i>
    </div>
    <!-- END FOOTER -->
    @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "LogOffForm" }))
